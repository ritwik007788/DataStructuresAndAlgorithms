// gfg

/* node of linked list:

class Node{
    int data;
    Node next;
    Node(int d){
        data=d;
        next=null;
    }
}

*/

class Solution{
    //Function to rotate a linked list.
    public Node rotate(Node head, int k) {
        // add code here
        Node temp = head;
        while(temp.next!=null){
            temp = temp.next;
        }
        temp.next = head;
        
        Node end = head;
        while(k!=1){
            end = end.next;
            k--;
        }
        Node res = end.next;
        end.next = null;
        return res;
    }
}
